
.MODEL SHALL
.STACK 100H
.DATA
 MASSG1 DB 0AH,0DH,'YOUR INPUT IS A VOWEL$' 
 MASSG2 DB 0AH,0DH,'YOUR INPUT IS NOT A VOWEL$'

.CODE
 MAIN PROC
MOV AX, @DATA
MOV DS, AX

MOV AH, 1 
INT 21H
CMP AL, 'a'
JE LEVEL1
CMP AL, 'e'
JE LEVEL1
CMP AL, 'i'
JE LEVEL1
CMP AL, 'o'
JE LEVEL1
CMP AL, 'u'
JE LEVEL1 
CMP AL, 'A'
JE LEVEL1
CMP AL, 'E'
JE LEVEL1
CMP AL, 'I'
JE LEVEL1
CMP AL, 'O'
JE LEVEL1
CMP AL, 'U'
JE LEVEL1
JNE LEVEL2

LEVEL1: 
MOV DL, 'V'
MOV AH, 2 
LEA DX, MASSG1
MOV AH, 9
INT 21H   

JMP EXIT
LEVEL2: 
MOV DL, 'C'
MOV AH, 2 
LEA DX, MASSG2
MOV AH, 9
INT 21H 

EXIT: 
MOV AH, 4CH
INT 21H
